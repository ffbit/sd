html
  head
    title= title
    link(rel='icon' href='data:image/jpeg;base64,')
  body
    h1= title
  div
    ul
      each link in links
        li
          a(href='/' + link + '.html')= link
  dif(class='footer')
    a(href='/index.html') Home
    span -
    a(href='/' + title + '-broken.html') A broken link
    span -
    a(href='/just-broken.html') Another broken link 
    span -
    a(href='https://example.org/index.html') An external link
    span -
    a(href='https://example.org/index.html?msg=hello') An external link
